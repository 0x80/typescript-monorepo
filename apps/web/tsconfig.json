{
  "extends": "@codecompose/typescript-config/nextjs",
  /* This is injected by Next because v15.5 does not yet understand that it comes from extends already */
  "include": [
    "${configDir}",
    "${configDir}/**/*.json",
    "${configDir}/next-env.d.ts",
    "${configDir}/.next/types/**/*.ts",
    ".next/types/**/*.ts"
  ],
  "references": [
    {
      "path": "../../packages/common"
    }
  ],
}
